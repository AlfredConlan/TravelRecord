    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <ListView x:Name="postListView">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Experience}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>